<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>

<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="controller.AdminController"
      alignment="center" spacing="10"
      styleClass="root">

    <Button text="Add Patron" onAction="#addPatron"/>
    <Button text="Remove Patron" onAction="#removePatron"/>
    <Button text="Add Book" onAction="#addBook"/>
    <Button text="Remove Book" onAction="#removeBook"/>
    <Button text="Exit Administration" onAction="#exitAdministration"/>

    <stylesheets>
        <URL value="@style.css"/>
    </stylesheets>
</VBox>
